            <stringProp name="script">import com.alibaba.fastjson.JSON;
import com.alibaba.fastjson.JSONArray; 
import com.alibaba.fastjson.JSONObject;


String materialId=&quot;&quot;;
try{

String resObj = prev.getResponseDataAsString();

JSONObject responseObj = JSON.parseObject(resObj); 
JSONArray listArray = responseObj.getJSONObject(&quot;data&quot;).getJSONArray(&quot;rows&quot;);

for(int i=0;i&lt;listArray.size();i++){
	
	 String materialStatus = listArray.getJSONObject(i).getString(&quot;materialStatus&quot;);
	 String materialType = listArray.getJSONObject(i).getString(&quot;materialType&quot;);
	 if(materialStatus.equals(&quot;usable&quot;) &amp;&amp; materialType.equals(&quot;video&quot;)){
	 	materialId = listArray.getJSONObject(i).getString(&quot;materialId&quot;);
	 	break;
	 }else
	 continue;
	 
	}


}catch (Throwable ex) {
    log.error(&quot;Failed!&quot;, ex);
}
log.info(&quot;materialId:&quot;+materialId);
vars.put(&quot;materialId&quot;,materialId);</stringProp>
          </BeanShellPostProcessor>
